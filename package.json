{
  "name": "notion-blog",
  "version": "0.1.0",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider next build && cross-env NODE_OPTIONS=--openssl-legacy-provider tsx src/lib/build-rss.ts",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .gitignore",
    "lint-staged": "lint-staged",
    "postinstall": "node scripts/remove-nested-react-types.cjs"
  },
  "pre-commit": "lint-staged",
  "dependencies": {
    "@react95/icons": "^2.2.0",
    "@notionhq/client": "^2.2.15",
    "async-sema": "3.1.0",
    "github-slugger": "1.2.1",
    "katex": "0.12.0",
    "next": "13.5.6",
    "prismjs": "1.17.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-jsx-parser": "^2.0.0",
    "uuid": "8.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/katex": "0.11.0",
    "@types/node": "20.10.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "^18.2.0",
    "cross-env": "^10.1.0",
    "lint-staged": "10.5.4",
    "pre-commit": "1.2.2",
    "prettier": "2.2.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "resolutions": {
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0"
  },
  "overrides": {
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "react-jsx-parser": {
      "@types/react": "18.2.0"
    }
  }
}
